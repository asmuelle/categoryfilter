<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='categoryfilter'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>
  <inherits name='com.google.gwt.visualization.Visualization'/>
   
  <inherits name="com.google.gwt.user.theme.chrome.ChromeResources"/>
  <!-- Specify the app entry point class.                         -->
  <entry-point class='org.herban.categoryfilter.client.CategoryFilter'/>
 <stylesheet src='gwt/chrome/chrome.css'/>
  <stylesheet src='CategoryFilter.css'/>
  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
  
</module>
